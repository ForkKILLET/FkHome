[credential]
	helper = store
[core]
	editor = env vim
[alias]
	m	= !vim ~/.gitconfig		; Map
	l 	= log --graph --all
	lo	= log --graph --all --oneline
	s	= status
	d 	= diff
	dc	= diff --cached
	a 	= add
	aa  = add .
	rc	= rm --cached
	c	= commit
	ca	= commit --amend
	co	= checkout
	b	= branch
	bs 	= switch
	dl 	= clone					; Download
	p 	= push
	b	= branch
	ig	= !vim ./.gitignore && git a .gitignore

